<script>
    $(document).ready(function () {
        var tabs = document.querySelectorAll('.tab');
        for (var i = 0; i < tabs.length; i++) {
            tabs[i].addEventListener('click', (el) => {
                var req = document.querySelectorAll('.tab')
                for (var j = 0; j < req.length; j++) {
                    document.getElementById("" + req[j].getAttribute('data-target')).style.visibility = "hidden";
                    document.getElementById("" + req[j].getAttribute('data-target')).style.display = "none";
                    req[j].classList.remove('tab-active')


                }
                console.log(el)
                document.getElementById("" + el.target.getAttribute('data-target')).style.visibility = "visible";
                el.target.classList.add('tab-active')
                document.getElementById("" + el.target.getAttribute('data-target')).style.display = "block";

            })
        }
    })
</script>
